@using BeSpoked;
@{
    var employeeList = (List<EmployeeModel>)ViewData["EmployeeList"];
}


<h1>Employees</h1>
<a class="btn btn-sm btn-primary" asp-action="Create" asp-controller="Employee"> Add</a>

<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in employeeList)
        {
            <tr>
                <td>@employee.Em_Name_First</td>
                <td>@employee.Em_Name_Last</td>
            </tr>
        }
    </tbody>
</table>
